{
  "name": "@nanduland/cli",
  "description": "Nandu Open NPM Registry CLI",
  "version": "1.0.0",
  "author": "@taskforcesh",
  "bin": {
    "nandu": "./bin/run"
  },
  "bugs": "https://github.com/taskforcesh/nandu/issues",
  "dependencies": {
    "@nanduland/service": "workspace:1.0.0",
    "@npmcli/config": "^2.4.0",
    "@oclif/command": "^1.8.16",
    "@oclif/config": "^1.18.3",
    "@oclif/plugin-help": "^3.3.1",
    "axios": "^0.24.0",
    "cli-ux": "^6.0.9"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/test": "^1.2.9",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^16.11.58",
    "chai": "^4.3.6",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/taskforcesh/nandu",
  "keywords": [
    "oclif"
  ],
  "license": "AGPL",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nandu",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "token:create": {
        "description": "create a new token for given user"
      },
      "token:ls": {
        "description": "list tokens for given user"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taskforcesh/nandu.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/gitbook/changelog-cli.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "pnpm-lock.yaml",
            "docs/gitbook/changelog.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}

{
  "name": "@nandu/service",
  "version": "1.0.3",
  "description": "Open NPM Registry",
  "engines": {
    "node": ">=14"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "npm",
    "registry"
  ],
  "scripts": {
    "test": "test",
    "dev2": "export $(grep -v '^#' .env | xargs)",
    "dev": "export $(grep -v '^#' .env | xargs) && nodemon --watch './src/**/*.ts' --exec 'ts-node' ./server.ts | pino-pretty",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepare": "npm run build"
  },
  "author": "Taskforce.sh Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/taskforcesh/nandu.git"
  },
  "license": "AGPL",
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.0.0",
    "@aws-sdk/client-s3": "^3.41.0",
    "@aws-sdk/lib-storage": "^3.41.0",
    "@nandu/dashboard": "1.0.3",
    "@types/nodemailer": "^6.4.6",
    "@types/nodemailer-mailgun-transport": "^1.4.3",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-jwt": "^7.7.5",
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.1.4",
    "is-jwt": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.8.0",
    "nodemailer-mailgun-transport": "^2.1.5",
    "passport": "^0.6.0",
    "pg": "^8.8.0",
    "pino": "^7.1.0",
    "range_check": "^2.0.4",
    "sequelize": "^6.28.0",
    "sqlite3": "^5.0.2",
    "umzug": "^3.2.1",
    "uuid": "^8.3.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "*",
    "@types/sqlite3": "^3.1.7",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.6",
    "nodemon": "^2.0.14",
    "pino-pretty": "^8.1.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "4.4.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/gitbook/changelog.md"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "pnpm-lock.yaml",
            "docs/gitbook/changelog.md"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
